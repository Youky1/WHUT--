/*信息类别（联系方式、校区）*/
.message-title {
    display: inline-block;
    color: #9b9b9b;
    margin-bottom: 20rpx;
    font-weight: 600;
}

/*包裹每一种信息和其输入的容器*/
.message-box {
    width: 93%;
    margin: 17rpx 0 10rpx 0;
    padding-bottom: 20rpx;
    border-bottom: 1px solid #ededed;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

/*价格等信息的提示部分*/
.box-title {
    font-size: 14px;
    font-weight: 700;
    line-height: 100%;
}

/*信息输入部分（input框和picker）*/
.box-input    {
    width: 250rpx;
    display: inline-block;
}

/*物品名称输入框*/
.descri-text {
    margin-top: 30rpx;
    height: 200rpx;
    width: 90%;
    letter-spacing: 1px;
    font-size: 14px;
    line-height: 20px;
    vertical-align: 17px;
}
  
/* 灰色分隔线 */
.greyBlank {
  height: 22rpx;
  background-color: #f1f1f1;
}


